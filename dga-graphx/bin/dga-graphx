#! /bin/bash

T="$(date +%s)"
java -cp "/opt/spark-1.0.0/lib_managed/jars/*:/opt/spark-1.0.0/assembly/target/scala-2.10/*:jars/*" com.soteradefense.dga.graphx.DGARunner "$@" --jars "jars/dga-graphx-0.1.jar,jars/dga-core-0.0.1.jar,jars/spark-graphx_2.10-1.0.0.jar"

T="$(($(date +%s)-T))"
echo "Time in seconds: ${T}"