#! /bin/bash

export DGA_CLASSPATH=lib/*:conf/
export SPARK_JARS_ASSEMBLY=/opt/spark-1.0.0/lib_managed/jars/*:/opt/spark-1.0.0/assembly/target/scala-2.10/*

T="$(date +%s)"
java -cp $DGA_CLASSPATH:$SPARK_JARS_ASSEMBLY com.soteradefense.dga.graphx.DGARunner "$@" --jars "lib/dga-graphx-0.1.jar,lib/dga-core-0.0.1.jar,lib/spark-graphx_2.10-1.0.0.jar"

T="$(($(date +%s)-T))"
echo "Time in seconds: ${T}"